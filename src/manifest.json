{
  "manifest_version": 2,
  "name": "Surprise Dora",
  "description": "Top secret Google Chrome extension for my Dora",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": 
      [
      	"http://www.google.com/*",
      	"https://www.google.com/*",
      	"http://www.google.com.ph/*",
      	"https://www.google.com.ph/*"
      ],
      "js": ["js/jquery-2.1.1.min.js", "js/myscript_google.js"],
      "css": ["css/google.css"]
    }
  ]
}